{
  "name": "spotify-web-api-ts",
  "version": "1.0.0",
  "description": "An isomorphic TypeScript wrapper for Spotify's Web API",
  "main": "cjs/SpotifyWebApi.js",
  "module": "esm/SpotifyWebApi.js",
  "types": "types",
  "repository": "https://github.com/adamgrieger/spotify-web-api-ts.git",
  "author": "Adam Grieger",
  "license": "MIT",
  "files": [
    "cjs",
    "esm",
    "types"
  ],
  "scripts": {
    "build": "web-scripts build --project tsconfig.build.json",
    "commit": "web-scripts commit",
    "format": "web-scripts format",
    "lint": "web-scripts lint",
    "prepare": "web-scripts audit",
    "release": "web-scripts release",
    "test": "web-scripts test",
    "generate-docs": "typedoc --out docs src/SpotifyWebApi.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "web-scripts commitmsg",
      "pre-commit": "web-scripts precommit"
    }
  },
  "devDependencies": {
    "@spotify/web-scripts": "^7.0.1",
    "@types/qs": "^6.9.3",
    "husky": "^4.2.5",
    "typedoc": "^0.17.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "qs": "^6.9.4"
  }
}
